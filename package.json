{
  "name": "lillego-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript and Discord.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "tsc && node dist/index.js",
    "deploy": "tsc && node dist/deploy-commands.js",
    "clear": "tsc && node dist/clear-commands.js",
    "redeploy": "npm run clear && npm run deploy"
  },
  "keywords": ["discord", "bot", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.22.1",
    "dotenv": "^16.3.1",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0"
  }
}
