{
  "name": "lillego-bot",
  "version": "1.0.0",
  "description": "A Discord bot built with TypeScript and Discord.js",
  "main": "dist/index.js",
  "scripts": {
    "build:ts": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "deploy": "node dist/deploy-commands.js",
    "clear": "node dist/clear-commands.js",
    "redeploy": "npm run clear && npm run deploy",
    "build:app": "npm install && npm run build:ts && npm run redeploy"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.22.1",
    "dotenv": "^16.3.1",
    "@types/express": "^4.17.23",
    "express": "^5.1.0",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
